<server>
	<world>
		<!-- Set the shortname to ONE word. The longname is what shows up on server list -->
		<shortname>MTST</shortname>
		<longname>My Test Server</longname>

		<!-- DO NOT EDIT ANY LINES BETWEEN HERE AND THE DATABASE SECTION -->
		<!-- <address>127.0.0.1</address> -->
		<!-- <localaddress>127.0.0.1</localaddress> -->

		<!-- Loginserver information. DO NOT EDIT -->
		<loginserver>
			<host>192.168.5.60</host>
			<port>5998</port>
			<account>Admin</account>
			<password>Password</password>
		</loginserver>


		<!-- Server status. Default is unlocked DO NOT EDIT RIGHT NOW -->
		<!--<locked/>-->
		<!-- <unlocked/> -->

		<tcp ip="127.0.0.1" port="9001"/>
		<telnet ip="0.0.0.0" port="9000" enabled="true"/>

		<!-- Sets the shared key used by zone/launcher to connect to world -->
		<key>somelongrandomstring12345asdfffff</key>

		<!-- Enable and set the port for the HTTP service. Defaults are shown -->
		<!-- <http port="9080" enabled="false" mimefile="mime.types" /> -->
		<http port="9080" enabled="true" mimefile="mime.types" />
	</world>

	<!-- Chatserver (channels) information. DO NOT EDIT -->
	<chatserver>
		<host></host>
		<port>7500</port>
	</chatserver>

	<!-- Mailserver (in-game mail) information. DO NOT EDIT -->
	<mailserver>
		<host></host>
		<port>7500</port>
	</mailserver>

	<zones>
	<!-- The defaultstatus is what status the new toons will have on your server -->
	<defaultstatus>0</defaultstatus>

	<!-- Sets port range for world to use to auto configure zones DO NOT EDIT RIGHT NOW-->
	<!-- <ports low="7000" high="7100"/> -->
	<ports low="7000" high="7400"/>
	</zones>

	<!-- Set username to root and password is your MySQL password and db to peq -->
	<database>
	   <host>192.168.5.10</host>
	   <port>3306</port>
	   <username>user1</username>
	   <password>1234567</password>
	   <db>stuff</db>
	</database>	

	<qsdatabase>
	   <host>192.168.5.10</host>
	   <port>3306</port>
	   <username>user1</username>
	   <password>1234567</password>
	   <db>stuff</db>
	</qsdatabase>

	<webinterface>
		<port>9081</port>
	</webinterface>

